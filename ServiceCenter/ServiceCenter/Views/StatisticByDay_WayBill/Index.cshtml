    <script src="@Url.Content("~/Highcharts/js/highcharts.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Highcharts/js/modules/exporting.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/JS/StatisticByDay_WayBill.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/JqueryUI/easyui-lang-zh_CN.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/My97DatePicker/WdatePicker.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/JS/Common/Common.js")" type="text/javascript"></script>
</head>
<body>
    <div>
        @Html.GetLangbyKey("StatisticByDay_WayBill_Html_Label1"):<input type="text" id="txtDate_Begin" name="txtDate_Begin" onfocus="WdatePicker({dateFmt:'yyyy-MM-dd'});" class="Wdate"  value='@(DateTime.Now.ToString("yyyy-MM-01"))' style="width:100px"/>
        &nbsp;&nbsp;&nbsp;@Html.GetLangbyKey("StatisticByDay_WayBill_Html_Label2"):<input type="text" id="txtDate_End" name="txtDate_End" onfocus="WdatePicker({dateFmt:'yyyy-MM-dd'});" class="Wdate"  value='@(DateTime.Now.ToString("yyyy-MM-dd"))' style="width:100px"/>&nbsp;&nbsp;@Html.GetLangbyKey("StatisticByDay_WayBill_Html_Label3"):<input
            type="text" id="ddlStation" name="ddlStation" />&nbsp;&nbsp;&nbsp;@Html.GetLangbyKey("StatisticByDay_WayBill_Html_ddlEmpNO"):<input type="text"
                id="ddlEmpNO" name="ddlEmpNO" />
        <a href="#" class="easyui-linkbutton" id="btnQuery" iconcls="icon-search" plain="false">
            @Html.GetLangbyKey("StatisticByDay_WayBill_Html_Label4")</a>
    </div>
    <div id="container" style="min-width: 310px; height: 460px; margin: 0 auto">
    </div>
</body>
</html>
