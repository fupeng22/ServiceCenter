            <a href="#" class="easyui-linkbutton" id="btnQuery" iconcls="icon-search" plain="true">
                @Html.GetLangbyKey("QueryPic_Html_btnQuery")</a>|<a href="javascript:void(0)" id="menuButtonReport" class="easyui-menubutton"
                    data-options="menu:'#menuButtons',iconCls:'icon-report'">@Html.GetLangbyKey("QueryPic_Html_menuButtonReport")</a>
            <div id="menuButtons" style="width: 150px;">
                <div id="btnPrint" name="btnPrint" data-options="iconCls:'icon-print'">
                    @Html.GetLangbyKey("QueryPic_Html_btnPrint")</div>
                <div id="btnExcel" name="btnExcel" data-options="iconCls:'icon-excel'">
                    @Html.GetLangbyKey("QueryPic_Html_btnExcel")</div>
            </div>
        </div>
        <div>
            @Html.GetLangbyKey("QueryPic_Html_txtLabel"):<input type="text" id="txtLabel" name="txtLabel" />&nbsp;&nbsp;&nbsp;
        </div>
    </div>
    <div id="DG_Pic" fit="true">
    </div>
