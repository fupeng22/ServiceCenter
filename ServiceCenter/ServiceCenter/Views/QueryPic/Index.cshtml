@Html.Raw(Html.LangOutJsVar("Public_Dialog_Save"));
@Html.Raw(Html.LangOutJsVar("Public_Dialog_Close"));

@Html.Raw(Html.LangOutJsVar("QueryPic_JS_Field1"));
