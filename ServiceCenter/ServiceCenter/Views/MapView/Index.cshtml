@using ServiceCenter.Models;
<script type="text/javascript">
@Html.Raw(LangHelper.WhichLang());
@Html.Raw(Html.LangOutJsVar("Default_JS_defaultTabTitle"));
@Html.Raw(Html.LangOutJsVar("Public_Dialog_Title"));
@Html.Raw(Html.LangOutJsVar("Public_Dialog_AskForExit"));
@Html.Raw(Html.LangOutJsVar("Public_Dialog_NoDataForPrint"));
@Html.Raw(Html.LangOutJsVar("Public_Dialog_NoDataForExcel"));
@Html.Raw(Html.LangOutJsVar("Public_Print_Title"));
@Html.Raw(Html.LangOutJsVar("Public_Dialog_Save"));
@Html.Raw(Html.LangOutJsVar("Public_Dialog_Close"));

@Html.Raw(Html.LangOutJsVar("MapView_JS_AddMapPot"));
@Html.Raw(Html.LangOutJsVar("MapView_JS_UpdateMapPot"));
@Html.Raw(Html.LangOutJsVar("MapView_JS_Field1"));
@Html.Raw(Html.LangOutJsVar("MapView_JS_Field2"));
@Html.Raw(Html.LangOutJsVar("MapView_JS_ErrorTip1"));
@Html.Raw(Html.LangOutJsVar("MapView_JS_ErrorTip2"));
@Html.Raw(Html.LangOutJsVar("MapView_JS_ErrorTip3"));
</script>
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@Html.GetLangbyKey("MapView_Html_Title")</title>
    <script src="@Url.Content("~/Scripts/jquery-1.6.2.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/JqueryUI/jquery.easyui.min.js")" type="text/javascript"></script>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/black/easyui.css")" rel="stylesheet"
        title="black" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/bootstrap/easyui.css")" rel="stylesheet"
        title="bootstrap"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/default/easyui.css")" rel="stylesheet"
        title="default"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/gray/easyui.css")" rel="stylesheet"
        title="gray"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/metro/easyui.css")" rel="stylesheet"
        title="metro"/>
    <link href="@Url.Content("~/JqueryUI/themes/icon.css")"  rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Highcharts/js/highcharts.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Highcharts/js/modules/exporting.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/JS/MapView.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/JqueryUI/easyui-lang-zh_CN.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/My97DatePicker/WdatePicker.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/JS/Common/Common.js")" type="text/javascript"></script>
    <link href="@Url.Content("~/Css/MapPot.css")"  rel="stylesheet"   type="text/css" />

</head>
<body id="body_Main" style="margin: 0px; position: absolute;">
    <img src="" id="img_Map" />
    <div id="menuForPots" class="easyui-menu" style="width: 200px;">
        <div id="mnu_AddMapPort" name="mnu_AddMapPort" iconcls="icon-add">
            @Html.GetLangbyKey("MapView_Html_Menu1")</div>
    </div>
    <div id="editMapPortMemu" class="easyui-menu" style="width: 200px;">
        <div id="mnu_UpdateMapPort_Detail" name="mnu_UpdateMapPort_Detail" iconcls="icon-edit">
            @Html.GetLangbyKey("MapView_Html_Menu2")</div>
        <div id="mnu_DeleMapPort_Detail" name="mnu_DeleMapPort_Detail" iconcls="icon-remove">
            @Html.GetLangbyKey("MapView_Html_Menu3")</div>
        <div id="mnu_ViewMapPort_Detail" name="mnu_ViewMapPort_Detail">
            @Html.GetLangbyKey("MapView_Html_Menu4")</div>
    </div>
    <div id="div_MapPotAdd" style="padding: 2px">
        <table>
            <tr>
                <td>
                    @Html.GetLangbyKey("MapView_Html_Tip1"):
                </td>
                <td colspan="7">
                    <font style="color: Red; font-weight: bold"><span id="span_GroupName_Add" name="span_GroupName_Add">
                    </span></font>
                </td>
            </tr>
            <tr>
                <td>
                    @Html.GetLangbyKey("MapView_Html_Tip2"):
                </td>
                <td colspan="7">
                    <font style="color: Red; font-weight: bold"><span id="span_MapNameForMapPort_Add"
                        name="span_MapNameForMapPort_Add"></span></font>
                </td>
            </tr>
            <tr>
                <td>
                    @Html.GetLangbyKey("MapView_Html_Tip3"):
                </td>
                <td>
                    <input type="text" id="txtMapPortXPos_Add" name="txtMapPortXPos_Add" style="width: 50px"
                        readonly />
                </td>
                <td>
                    @Html.GetLangbyKey("MapView_Html_Tip4"):
                </td>
                <td>
                    <input type="text" id="txtMapPortYPos_Add" name="txtMapPortYPos_Add" style="width: 50px"
                        readonly />
                </td>
                <td style="display: none">
                    @Html.GetLangbyKey("MapView_Html_Tip5"):
                </td>
                <td style="display: none">
                    <input type="text" id="txtMapPortWidth_Add" name="txtMapPortWidth_Add" style="width: 50px"
                        value="30" />
                </td>
                <td style="display: none">
                    @Html.GetLangbyKey("MapView_Html_Tip6"):
                </td>
                <td style="display: none">
                    <input type="text" id="txtMapPortHeight_Add" name="txtMapPortHeight_Add" style="width: 50px"
                        value="40" />
                </td>
            </tr>
            <tr>
                <td>
                    @Html.GetLangbyKey("MapView_Html_Tip7"):
                </td>
                <td colspan="7">
                    <input type="text" id="txtMapPortCustomizeName_Add" name="txtMapPortCustomizeName_Add"
                        style="width: 340px" />
                </td>
            </tr>
            <tr>
                <td>
                    @Html.GetLangbyKey("MapView_Html_Tip8"):
                </td>
                <td colspan="7">
                    <div id="CGEquipmentForMapPort_Add" style="width: 340px">
                    </div>
                </td>
            </tr>
            <tr>
                <td colspan="8">
                    <font style="color: Blue; font-weight: bold"><span>@Html.GetLangbyKey("MapView_Html_Tip9")</span></font>
                </td>
            </tr>
        </table>
    </div>
    <div id="div_MapPotUpdate" style="padding: 2px">
        <table>
            <tr>
                <td>
                    @Html.GetLangbyKey("MapView_Html_Tip1"):
                </td>
                <td colspan="7">
                    <font style="color: Red; font-weight: bold"><span id="span_GroupName_Update" name="span_GroupName_Update">
                    </span></font>
                    <input type="hidden" id="hid_MapPotIdForUpdate" name="hid_MapPotIdForUpdate" />
                </td>
            </tr>
            <tr>
                <td>
                    @Html.GetLangbyKey("MapView_Html_Tip2"):
                </td>
                <td colspan="7">
                    <font style="color: Red; font-weight: bold"><span id="span_MapNameForMapPort_Update"
                        name="span_MapNameForMapPort_Update"></span></font>
                </td>
            </tr>
            <tr>
                <td>
                    @Html.GetLangbyKey("MapView_Html_Tip3"):
                </td>
                <td>
                    <input type="text" id="txtMapPortXPos_Update" name="txtMapPortXPos_Update" style="width: 50px;"
                        readonly />
                </td>
                <td>
                    @Html.GetLangbyKey("MapView_Html_Tip4"):
                </td>
                <td>
                    <input type="text" id="txtMapPortYPos_Update" name="txtMapPortYPos_Update" style="width: 50px"
                        readonly />
                </td>
                <td style="display: none">
                    @Html.GetLangbyKey("MapView_Html_Tip5"):
                </td>
                <td style="display: none">
                    <input type="text" id="txtMapPortWidth_Update" name="txtMapPortWidth_Update" style="width: 50px" />
                </td>
                <td style="display: none">
                    @Html.GetLangbyKey("MapView_Html_Tip6"):
                </td>
                <td style="display: none">
                    <input type="text" id="txtMapPortHeight_Update" name="txtMapPortHeight_Update" style="width: 50px" />
                </td>
            </tr>
            <tr>
                <td>
                    @Html.GetLangbyKey("MapView_Html_Tip7"):
                </td>
                <td colspan="7">
                    <input type="text" id="txtMapPortCustomizeName_Update" name="txtMapPortCustomizeName_Update"
                        style="width: 340px" />
                </td>
            </tr>
            <tr>
                <td>
                    @Html.GetLangbyKey("MapView_Html_Tip8"):
                </td>
                <td colspan="7">
                    <div id="CGEquipmentForMapPort_Update" style="width: 340px">
                    </div>
                </td>
            </tr>
            <tr>
                <td colspan="8">
                    <font style="color: Blue; font-weight: bold"><span>@Html.GetLangbyKey("MapView_Html_Tip9")</span></font>
                </td>
            </tr>
        </table>
    </div>
    <input type="hidden" id="hid_MapId" name="hid_MapId" runat="server"  value="@ViewData["MapId"]"/>
    <input type="hidden" id="hid_MapName" name="hid_MapName" runat="server"  value="@ViewData["MapName"]"/>
    <input type="hidden" id="hid_MapPath" name="hid_MapPath" runat="server"  value="@ViewData["MapPath"]"/>
    <input type="hidden" id="hid_GroupName" name="hid_GroupName" runat="server"  value="@ViewData["GroupName"]"/>
</body>
</html>
