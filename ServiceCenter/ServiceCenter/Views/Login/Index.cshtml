@using ServiceCenter.Models;
<script type="text/javascript">
@Html.Raw(LangHelper.WhichLang());
@Html.Raw(Html.LangOutJsVar("Default_JS_defaultTabTitle"));
@Html.Raw(Html.LangOutJsVar("Public_Dialog_Title"));
@Html.Raw(Html.LangOutJsVar("Public_Dialog_AskForExit"));
@Html.Raw(Html.LangOutJsVar("Login_JS_ErrorTip1"));
</script>
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@Html.GetLangbyKey("Login_Html_Title")</title>
    <script src="@Url.Content("~/Scripts/jquery-1.6.2.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/JqueryUI/jquery.easyui.min.js")" type="text/javascript"></script>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/black/easyui.css")" rel="stylesheet"
        title="black" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/bootstrap/easyui.css")" rel="stylesheet"
        title="bootstrap"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/default/easyui.css")" rel="stylesheet"
        title="default"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/gray/easyui.css")" rel="stylesheet"
        title="gray"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/JqueryUI/themes/metro/easyui.css")" rel="stylesheet"
        title="metro"/>
    <link href="@Url.Content("~/JqueryUI/themes/icon.css")"  rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Highcharts/js/highcharts.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Highcharts/js/modules/exporting.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/JS/Login.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/JqueryUI/easyui-lang-zh_CN.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/My97DatePicker/WdatePicker.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/JS/Common/Common.js")" type="text/javascript"></script>
    <link href="@Url.Content("~/images/Login/login.css")" rel="stylesheet" type="text/css" />
</head>
<body>
    <input type="hidden" id="hidRetURL" name="hidRetURL"  value='@Request.QueryString["URLRet"]'/>
    <div id="login">
        <div id="top">
            <div id="top_left">
                <img src='@Url.Content("~/images/Login/login_03.gif")' /></div>
            <div id="top_center">
            </div>
        </div>
        <div id="center">
            <div id="center_left">
            </div>
            <table id="center_middle">
                <tr>
                    <td align="right">
                        @Html.GetLangbyKey("Login_Html_Label1")
                    </td>
                    <td align="left">
                        <input type="text" name="txtUserName" id="txtUserName" />
                    </td>
                </tr>
                <tr>
                    <td align="right">
                        @Html.GetLangbyKey("Login_Html_Label2")
                    </td>
                    <td align="left">
                        <input type="password" name="txtUserPassword" id="txtUserPassword" />
                    </td>
                </tr>
                <tr align="center">
                    <td colspan="2">
                        <a href="#" class="easyui-linkbutton" id="btnLogin" iconcls="icon-login" plain="false">
                            @Html.GetLangbyKey("Login_Html_Label3")</a>&nbsp;&nbsp;&nbsp;<a href="#" class="easyui-linkbutton"
                                id="btnClear" iconcls="icon-reset" plain="false"> @Html.GetLangbyKey("Login_Html_Label4")</a>
                    </td>
                </tr>
            </table>
        </div>
        @*<div id="center_middle">
            <div id="user">
            </div>
            <div id="password">
            </div>
            <div id="btn">
            </div>
            <div id="center_right">
            </div>
        </div>*@
        <div id="down">
            <div id="down_left">
                <div id="inf">
                    <span class="inf_text">@Html.GetLangbyKey("Login_Html_Label5")</span> <span class="copyright">@Html.GetLangbyKey("Login_Html_Label6")</span>
                </div>
            </div>
            <div id="down_center">
            </div>
        </div>
    </div>
</body>
</html>
